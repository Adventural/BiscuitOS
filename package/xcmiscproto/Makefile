################################################
#
# xcmiscproto
#
################################################
XCMISCPROTO_NAME		:= xcmiscproto
XCMISCPROTO_VERSION		:= $(CONFIG_XCMISCPROTO_VERSION)
XCMISCPROTO_SITE		:= $(CONFIG_XCMISCPROTO_URL)
XCMISCPROTO_GITHUB		:= 
XCMISCPROTO_SUBNAME		:= 
XCMISCPROTO_DIR			:= $(srctree)/package/xcmiscproto
XCMISCPROTO_PATCH		:= $(srctree)/package/xcmiscproto/patch
XCMISCPROTO_DEMO		:= $(srctree)/package/xcmiscproto/demo.c
XCMISCPROTO_OUT			:= 
XCMISCPROTO_SCRIPTS		:= $(srctree)/scripts/package/GNU-package.sh
XCMISCPROTO_TAR			:= $(CONFIG_XCMISCPROTO_TAR)
XCMISCPROTO_PACKAGE		:= 
XCMISCPROTO_CONFIG		:=
SUB_TARGET			+= $(XCMISCPROTO_DIR)/build

XCMISCPROTO_sh = $(CONFIG_SHELL) $(XCMISCPROTO_SCRIPTS) \
               $1 $(XCMISCPROTO_NAME)X $(XCMISCPROTO_VERSION)X \
               $(XCMISCPROTO_SITE)X $(XCMISCPROTO_GITHUB)X \
               $(XCMISCPROTO_PATCH)X 5.0 \
	       $(CONFIG_XCMISCPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(XCMISCPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(XCMISCPROTO_SUBNAME)X \
               $(XCMISCPROTO_CONFIG)X $(CONFIG_XCMISCPROTO_CONFIG)X \
	       $(CONFIG_XCMISCPROTO_LDFLAGS) $(CONFIG_XCMISCPROTO_CFLAGS)

$(XCMISCPROTO_DIR)/build: FORCE
	$(Q)$(call XCMISCPROTO_sh,$(srctree))
