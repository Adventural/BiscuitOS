PK_BISCUITOS_MISC_DEMO_NAME        := BiscuitOS-MISC
PK_BISCUITOS_MISC_DEMO_VERSION     := $(CONFIG_PK_BISCUITOS_MISC_DEMO_VERSION)
PK_BISCUITOS_MISC_DEMO_SITE        := $(CONFIG_PK_BISCUITOS_MISC_DEMO_URL)
PK_BISCUITOS_MISC_DEMO_GITHUB      := $(CONFIG_PK_BISCUITOS_MISC_DEMO_GITHUB)
PK_BISCUITOS_MISC_DEMO_SUBNAME     :=
PK_BISCUITOS_MISC_DEMO_BUILDARCH   := $(CONFIG_PK_BISCUITOS_MISC_DEMO_BUILDARCH)
PK_BISCUITOS_MISC_DEMO_DIR         := $(srctree)/package/BiscuitOS-demo/BiscuitOS-MISC
PK_BISCUITOS_MISC_DEMO_PATCH       := $(PK_BISCUITOS_MISC_DEMO_DIR)/patch
PK_BISCUITOS_MISC_DEMO_BSBIT       := BiscuitOS-MISC-$(PK_BISCUITOS_MISC_DEMO_VERSION).bsbit
PK_BISCUITOS_MISC_DEMO_OUT         :=
PK_BISCUITOS_MISC_DEMO_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_BISCUITOS_MISC_DEMO_TAR         := $(CONFIG_PK_BISCUITOS_MISC_DEMO_TAR)
PK_BISCUITOS_MISC_DEMO_PACKAGE     :=
PK_BISCUITOS_MISC_DEMO_CONFIG      := $(CONFIG_PK_BISCUITOS_MISC_DEMO_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_MISC_DEMO_DIR)/build

PK_BISCUITOS_MISC_DEMO_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_MISC_DEMO_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_MISC_DEMO_NAME)X \
                 $(PK_BISCUITOS_MISC_DEMO_VERSION)X \
                 $(PK_BISCUITOS_MISC_DEMO_SITE)X \
                 $(PK_BISCUITOS_MISC_DEMO_GITHUB)X \
                 $(PK_BISCUITOS_MISC_DEMO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_MISC_DEMO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_MISC_DEMO_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_MISC_DEMO_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_MISC_DEMO_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_MISC_DEMO_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_MISC_DEMO_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_MISC_DEMO_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_MISC_DEMO_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_MISC_DEMO_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_MISC_DEMO_ASFLAGS)X \
                 $(PK_BISCUITOS_MISC_DEMO_BSBIT)X \
                 $(PK_BISCUITOS_MISC_DEMO_BUILDARCH)X \
		 $(PK_BISCUITOS_MISC_DEMO_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_MISC_DEMO_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_MISC_DEMO_CSRCFILE)

$(PK_BISCUITOS_MISC_DEMO_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_MISC_DEMO_sh,$(srctree))
