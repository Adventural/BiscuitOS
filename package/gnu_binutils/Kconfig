	default "http://ftp.gnu.org/pub/gnu/binutils/"
	string "url"

config GNU_BINUTILS_LDFLAGS
	string "LDFLAGS"

config GNU_BINUTILS_CFLAGS
	string "CFLAGS"

config GNU_BINUTILS_CONFIG
	string "configure"

endif # PK_GNU_BINUTILS
