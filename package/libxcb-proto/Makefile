################################################
#
# xcb-proto
#
################################################
LIB_LIBXCB_PROTO_NAME		:= xcb-proto
LIB_LIBXCB_PROTO_VERSION	:= $(CONFIG_LIB_LIBXCB_PROTO_VERSION)
LIB_LIBXCB_PROTO_SITE		:= $(CONFIG_LIB_LIBXCB_PROTO_URL)
LIB_LIBXCB_PROTO_GITHUB		:= 
LIB_LIBXCB_PROTO_SUBNAME	:= 
LIB_LIBXCB_PROTO_DIR		:= $(srctree)/package/xcb-proto
LIB_LIBXCB_PROTO_PATCH		:= $(srctree)/package/xcb-proto/patch
LIB_LIBXCB_PROTO_DEMO		:= $(srctree)/package/xcb-proto/demo.c
LIB_LIBXCB_PROTO_OUT		:= 
LIB_LIBXCB_PROTO_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBXCB_PROTO_TAR		:= $(CONFIG_LIB_LIBXCB_PROTO_TAR)
LIB_LIBXCB_PROTO_PACKAGE	:= 
LIB_LIBXCB_PROTO_CONFIG		:=
SUB_TARGET			+= $(LIB_LIBXCB_PROTO_DIR)/build

LIB_LIBXCB_PROTO_sh = $(CONFIG_SHELL) $(LIB_LIBXCB_PROTO_SCRIPTS) \
               $1 $(LIB_LIBXCB_PROTO_NAME)X $(LIB_LIBXCB_PROTO_VERSION)X \
               $(LIB_LIBXCB_PROTO_SITE)X $(LIB_LIBXCB_PROTO_GITHUB)X \
               $(LIB_LIBXCB_PROTO_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBXCB_PROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBXCB_PROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBXCB_PROTO_SUBNAME)X \
               $(LIB_LIBXCB_PROTO_CONFIG)X $(CONFIG_LIB_LIBXCB_PROTO_CONFIG)X \
	       $(CONFIG_LIB_LIBXCB_PROTO_LDFLAGS) $(CONFIG_LIB_LIBXCB_PROTO_CFLAGS)

$(LIB_LIBXCB_PROTO_DIR)/build: FORCE
	$(Q)$(call LIB_LIBXCB_PROTO_sh,$(srctree))
