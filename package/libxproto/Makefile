################################################
#
# xproto
#
################################################
LIB_LIBXPROTO_NAME	:= xproto
LIB_LIBXPROTO_VERSION	:= $(CONFIG_LIB_LIBXPROTO_VERSION)
LIB_LIBXPROTO_SITE	:= $(CONFIG_LIB_LIBXPROTO_URL)
LIB_LIBXPROTO_GITHUB	:= 
LIB_LIBXPROTO_SUBNAME	:= 
LIB_LIBXPROTO_DIR	:= $(srctree)/package/xproto
LIB_LIBXPROTO_PATCH	:= $(srctree)/package/xproto/patch
LIB_LIBXPROTO_DEMO	:= $(srctree)/package/xproto/demo.c
LIB_LIBXPROTO_OUT	:= 
LIB_LIBXPROTO_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBXPROTO_TAR	:= $(CONFIG_LIB_LIBXPROTO_TAR)
LIB_LIBXPROTO_PACKAGE	:= 
LIB_LIBXPROTO_CONFIG	:=
SUB_TARGET		+= $(LIB_LIBXPROTO_DIR)/build

LIB_LIBXPROTO_sh = $(CONFIG_SHELL) $(LIB_LIBXPROTO_SCRIPTS) \
               $1 $(LIB_LIBXPROTO_NAME)X $(LIB_LIBXPROTO_VERSION)X \
               $(LIB_LIBXPROTO_SITE)X $(LIB_LIBXPROTO_GITHUB)X \
               $(LIB_LIBXPROTO_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBXPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBXPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBXPROTO_SUBNAME)X \
               $(LIB_LIBXPROTO_CONFIG)X $(CONFIG_LIB_LIBXPROTO_CONFIG)X \
	       $(CONFIG_LIB_LIBXPROTO_LDFLAGS) $(CONFIG_LIB_LIBXPROTO_CFLAGS)

$(LIB_LIBXPROTO_DIR)/build: FORCE
	$(Q)$(call LIB_LIBXPROTO_sh,$(srctree))
