################################################
#
# libxfce4util
#
################################################
LIB_LIBXFCE4UTIL_NAME		:= libxfce4util
LIB_LIBXFCE4UTIL_VERSION	:= $(CONFIG_LIB_LIBXFCE4UTIL_VERSION)
LIB_LIBXFCE4UTIL_SITE		:= $(CONFIG_LIB_LIBXFCE4UTIL_URL)
LIB_LIBXFCE4UTIL_GITHUB		:= 
LIB_LIBXFCE4UTIL_SUBNAME	:= 
LIB_LIBXFCE4UTIL_DIR		:= $(srctree)/package/libxfce4util
LIB_LIBXFCE4UTIL_PATCH		:= $(srctree)/package/libxfce4util/patch
LIB_LIBXFCE4UTIL_DEMO		:= $(srctree)/package/libxfce4util/demo.c
LIB_LIBXFCE4UTIL_OUT		:= 
LIB_LIBXFCE4UTIL_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBXFCE4UTIL_TAR		:= $(CONFIG_LIB_LIBXFCE4UTIL_TAR)
LIB_LIBXFCE4UTIL_PACKAGE	:= 
LIB_LIBXFCE4UTIL_CONFIG		:=
SUB_TARGET			+= $(LIB_LIBXFCE4UTIL_DIR)/build

LIB_LIBXFCE4UTIL_sh = $(CONFIG_SHELL) $(LIB_LIBXFCE4UTIL_SCRIPTS) \
               $1 $(LIB_LIBXFCE4UTIL_NAME)X $(LIB_LIBXFCE4UTIL_VERSION)X \
               $(LIB_LIBXFCE4UTIL_SITE)X $(LIB_LIBXFCE4UTIL_GITHUB)X \
               $(LIB_LIBXFCE4UTIL_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBXFCE4UTIL_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBXFCE4UTIL_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBXFCE4UTIL_SUBNAME)X \
               $(LIB_LIBXFCE4UTIL_CONFIG)X $(CONFIG_LIB_LIBXFCE4UTIL_CONFIG)X \
	       $(CONFIG_LIB_LIBXFCE4UTIL_LDFLAGS) $(CONFIG_LIB_LIBXFCE4UTIL_CFLAGS)

$(LIB_LIBXFCE4UTIL_DIR)/build: FORCE
	$(Q)$(call LIB_LIBXFCE4UTIL_sh,$(srctree))
