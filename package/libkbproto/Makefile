################################################
#
# kbproto
#
################################################
LIB_LIBKBPROTO_NAME	:= kbproto
LIB_LIBKBPROTO_VERSION	:= $(CONFIG_LIB_LIBKBPROTO_VERSION)
LIB_LIBKBPROTO_SITE	:= $(CONFIG_LIB_LIBKBPROTO_URL)
LIB_LIBKBPROTO_GITHUB	:= 
LIB_LIBKBPROTO_SUBNAME	:= 
LIB_LIBKBPROTO_DIR	:= $(srctree)/package/kbproto
LIB_LIBKBPROTO_PATCH	:= $(srctree)/package/kbproto/patch
LIB_LIBKBPROTO_DEMO	:= $(srctree)/package/kbproto/demo.c
LIB_LIBKBPROTO_OUT	:= 
LIB_LIBKBPROTO_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBKBPROTO_TAR	:= $(CONFIG_LIB_LIBKBPROTO_TAR)
LIB_LIBKBPROTO_PACKAGE	:= 
LIB_LIBKBPROTO_CONFIG	:=
SUB_TARGET		+= $(LIB_LIBKBPROTO_DIR)/build

LIB_LIBKBPROTO_sh = $(CONFIG_SHELL) $(LIB_LIBKBPROTO_SCRIPTS) \
               $1 $(LIB_LIBKBPROTO_NAME)X $(LIB_LIBKBPROTO_VERSION)X \
               $(LIB_LIBKBPROTO_SITE)X $(LIB_LIBKBPROTO_GITHUB)X \
               $(LIB_LIBKBPROTO_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBKBPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBKBPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBKBPROTO_SUBNAME)X \
               $(LIB_LIBKBPROTO_CONFIG)X $(CONFIG_LIB_LIBKBPROTO_CONFIG)X \
	       $(CONFIG_LIB_LIBKBPROTO_LDFLAGS) $(CONFIG_LIB_LIBKBPROTO_CFLAGS)

$(LIB_LIBKBPROTO_DIR)/build: FORCE
	$(Q)$(call LIB_LIBKBPROTO_sh,$(srctree))
