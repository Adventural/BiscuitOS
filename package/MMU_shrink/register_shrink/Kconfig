	default "https://gitee.com/BiscuitOS_team/HardStack/raw/Gitee/Memory-Allocator/Shrink/register_shrink"
	string "url"

config PK_REGISTER_SHRINK_GITHUB
	string "GitHub"

config PK_REGISTER_SHRINK_SRCFILE
	default "main.c Kconfig Makefile"
	string "Source code list"

config PK_REGISTER_SHRINK_CSRCFILE
	string "Compile source code list"

config PK_REGISTER_SHRINK_CONFIG
	string "CONFIG: Project Configure Options"

config PK_REGISTER_SHRINK_LIBPATH
	string "LIBS: Libraries to pass to the linker, e.g. -l<library>"

config PK_REGISTER_SHRINK_CPPFLAGS
	string "CPPFLAGS: C/C++ preprocessor flags, e.g. -I<include dir>"

config PK_REGISTER_SHRINK_DPKCONFIG
	string "PKG_CONFIG_PATH: Directories to add to pkg-config's search path"

config PK_REGISTER_SHRINK_CFLAGS
	string "CFLAGS: C compiler flags"

config PK_REGISTER_SHRINK_LDFLAGS
	string "LDFLAGS: Linker flags"

config PK_REGISTER_SHRINK_CXXFLAGS
	string "CXXFLAGS: C++ compiler flags"

config PK_REGISTER_SHRINK_ASFLAGS
	string "CCASFLAGS: assembler compiler flags (defaults to CFLAGS)"

config PK_REGISTER_SHRINK_BUILDARCH
	string "--host options on configure for Architecture"

endif # PK_REGISTER_SHRINK
