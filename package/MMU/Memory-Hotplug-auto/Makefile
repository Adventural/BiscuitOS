PK_MMU_MEMORY_HOTPLUG_AUTO_NAME        := BiscuitOS-Memory-Hotplug-auto
PK_MMU_MEMORY_HOTPLUG_AUTO_VERSION     := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_VERSION)
PK_MMU_MEMORY_HOTPLUG_AUTO_SITE        := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_URL)
PK_MMU_MEMORY_HOTPLUG_AUTO_GITHUB      := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_GITHUB)
PK_MMU_MEMORY_HOTPLUG_AUTO_SUBNAME     :=
PK_MMU_MEMORY_HOTPLUG_AUTO_BUILDARCH   := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_BUILDARCH)
PK_MMU_MEMORY_HOTPLUG_AUTO_DIR         := $(srctree)/package/MMU/Memory-Hotplug-auto
PK_MMU_MEMORY_HOTPLUG_AUTO_PATCH       := $(PK_MMU_MEMORY_HOTPLUG_AUTO_DIR)/patch
PK_MMU_MEMORY_HOTPLUG_AUTO_BSBIT       := BiscuitOS-Memory-Hotplug-auto-$(PK_MMU_MEMORY_HOTPLUG_AUTO_VERSION).bsbit
PK_MMU_MEMORY_HOTPLUG_AUTO_OUT         :=
PK_MMU_MEMORY_HOTPLUG_AUTO_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MMU_MEMORY_HOTPLUG_AUTO_TAR         := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_TAR)
PK_MMU_MEMORY_HOTPLUG_AUTO_PACKAGE     :=
PK_MMU_MEMORY_HOTPLUG_AUTO_CONFIG      := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_CONFIG)
SUB_TARGET                  += $(PK_MMU_MEMORY_HOTPLUG_AUTO_DIR)/build

PK_MMU_MEMORY_HOTPLUG_AUTO_sh = $(CONFIG_SHELL) $(PK_MMU_MEMORY_HOTPLUG_AUTO_SCRIPTS) \
                 $1X \
                 $(PK_MMU_MEMORY_HOTPLUG_AUTO_NAME)X \
                 $(PK_MMU_MEMORY_HOTPLUG_AUTO_VERSION)X \
                 $(PK_MMU_MEMORY_HOTPLUG_AUTO_SITE)X \
                 $(PK_MMU_MEMORY_HOTPLUG_AUTO_GITHUB)X \
                 $(PK_MMU_MEMORY_HOTPLUG_AUTO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MMU_MEMORY_HOTPLUG_AUTO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MMU_MEMORY_HOTPLUG_AUTO_SUBNAME)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_CONFIG)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_LIBPATH)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_CPPFLAGS)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_DPKCONFIG)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_CFLAGS)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_LDFLAGS)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_CXXFLAGS)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_ASFLAGS)X \
                 $(PK_MMU_MEMORY_HOTPLUG_AUTO_BSBIT)X \
                 $(PK_MMU_MEMORY_HOTPLUG_AUTO_BUILDARCH)X \
		 $(PK_MMU_MEMORY_HOTPLUG_AUTO_CSRCFILE)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_SRCFILE)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_AUTO_CSRCFILE)

$(PK_MMU_MEMORY_HOTPLUG_AUTO_DIR)/build: FORCE
	$(Q)$(call PK_MMU_MEMORY_HOTPLUG_AUTO_sh,$(srctree))
