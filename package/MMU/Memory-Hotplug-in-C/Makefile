# BiscuitOS-Memory-Hotplug-in-C
#
################################################
PK_MMU_MEMORY_HOTPLUG_IN_C_NAME        := BiscuitOS-Memory-Hotplug-in-C
PK_MMU_MEMORY_HOTPLUG_IN_C_VERSION     := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_VERSION)
PK_MMU_MEMORY_HOTPLUG_IN_C_SITE        := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_URL)
PK_MMU_MEMORY_HOTPLUG_IN_C_GITHUB      := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_GITHUB)
PK_MMU_MEMORY_HOTPLUG_IN_C_SUBNAME     :=
PK_MMU_MEMORY_HOTPLUG_IN_C_BUILDARCH   := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_BUILDARCH)
PK_MMU_MEMORY_HOTPLUG_IN_C_DIR         := $(srctree)/package/MMU/Memory-Hotplug-in-C
PK_MMU_MEMORY_HOTPLUG_IN_C_PATCH       := $(PK_MMU_MEMORY_HOTPLUG_IN_C_DIR)/patch
PK_MMU_MEMORY_HOTPLUG_IN_C_BSBIT       := BiscuitOS-Memory-Hotplug-in-C-$(PK_MMU_MEMORY_HOTPLUG_IN_C_VERSION).bsbit
PK_MMU_MEMORY_HOTPLUG_IN_C_OUT         :=
PK_MMU_MEMORY_HOTPLUG_IN_C_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MMU_MEMORY_HOTPLUG_IN_C_TAR         := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_TAR)
PK_MMU_MEMORY_HOTPLUG_IN_C_PACKAGE     :=
PK_MMU_MEMORY_HOTPLUG_IN_C_CONFIG      := $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_CONFIG)
SUB_TARGET                  += $(PK_MMU_MEMORY_HOTPLUG_IN_C_DIR)/build

PK_MMU_MEMORY_HOTPLUG_IN_C_sh = $(CONFIG_SHELL) $(PK_MMU_MEMORY_HOTPLUG_IN_C_SCRIPTS) \
                 $1X \
                 $(PK_MMU_MEMORY_HOTPLUG_IN_C_NAME)X \
                 $(PK_MMU_MEMORY_HOTPLUG_IN_C_VERSION)X \
                 $(PK_MMU_MEMORY_HOTPLUG_IN_C_SITE)X \
                 $(PK_MMU_MEMORY_HOTPLUG_IN_C_GITHUB)X \
                 $(PK_MMU_MEMORY_HOTPLUG_IN_C_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MMU_MEMORY_HOTPLUG_IN_C_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MMU_MEMORY_HOTPLUG_IN_C_SUBNAME)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_CONFIG)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_LIBPATH)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_CPPFLAGS)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_DPKCONFIG)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_CFLAGS)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_LDFLAGS)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_CXXFLAGS)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_ASFLAGS)X \
                 $(PK_MMU_MEMORY_HOTPLUG_IN_C_BSBIT)X \
                 $(PK_MMU_MEMORY_HOTPLUG_IN_C_BUILDARCH)X \
		 $(PK_MMU_MEMORY_HOTPLUG_IN_C_CSRCFILE)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_SRCFILE)X \
                 $(CONFIG_PK_MMU_MEMORY_HOTPLUG_IN_C_CSRCFILE)

$(PK_MMU_MEMORY_HOTPLUG_IN_C_DIR)/build: FORCE
	$(Q)$(call PK_MMU_MEMORY_HOTPLUG_IN_C_sh,$(srctree))
