PK_BISCUITOS_MEMORY_0_11_NAME        := BiscuitOS_Memory
PK_BISCUITOS_MEMORY_0_11_VERSION     := $(CONFIG_PK_BISCUITOS_MEMORY_0_11_VERSION)
PK_BISCUITOS_MEMORY_0_11_SITE        := $(CONFIG_PK_BISCUITOS_MEMORY_0_11_URL)
PK_BISCUITOS_MEMORY_0_11_GITHUB      := $(CONFIG_PK_BISCUITOS_MEMORY_0_11_GITHUB)
PK_BISCUITOS_MEMORY_0_11_SUBNAME     :=
PK_BISCUITOS_MEMORY_0_11_BUILDARCH   := $(CONFIG_PK_BISCUITOS_MEMORY_0_11_BUILDARCH)
PK_BISCUITOS_MEMORY_0_11_DIR         := $(srctree)/package/memory_history/BiscuitOS/BiscuitOS_Memory-0.11
PK_BISCUITOS_MEMORY_0_11_PATCH       := $(PK_BISCUITOS_MEMORY_0_11_DIR)/patch
PK_BISCUITOS_MEMORY_0_11_BSBIT       := BiscuitOS_Memory-$(PK_BISCUITOS_MEMORY_0_11_VERSION).bsbit
PK_BISCUITOS_MEMORY_0_11_OUT         :=
PK_BISCUITOS_MEMORY_0_11_SCRIPTS     := $(srctree)/scripts/package/package-memory.sh
PK_BISCUITOS_MEMORY_0_11_TAR         := $(CONFIG_PK_BISCUITOS_MEMORY_0_11_TAR)
PK_BISCUITOS_MEMORY_0_11_PACKAGE     :=
PK_BISCUITOS_MEMORY_0_11_CONFIG      := $(CONFIG_PK_BISCUITOS_MEMORY_0_11_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_MEMORY_0_11_DIR)/build

PK_BISCUITOS_MEMORY_0_11_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_MEMORY_0_11_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_MEMORY_0_11_NAME)X \
                 $(PK_BISCUITOS_MEMORY_0_11_VERSION)X \
                 $(PK_BISCUITOS_MEMORY_0_11_SITE)X \
                 $(PK_BISCUITOS_MEMORY_0_11_GITHUB)X \
                 $(PK_BISCUITOS_MEMORY_0_11_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_MEMORY_0_11_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_MEMORY_0_11_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_MEMORY_0_11_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_MEMORY_0_11_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_MEMORY_0_11_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_MEMORY_0_11_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_MEMORY_0_11_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_MEMORY_0_11_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_MEMORY_0_11_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_MEMORY_0_11_ASFLAGS)X \
                 $(PK_BISCUITOS_MEMORY_0_11_BSBIT)X \
                 $(PK_BISCUITOS_MEMORY_0_11_BUILDARCH)X \
		 $(PK_BISCUITOS_MEMORY_0_11_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_MEMORY_0_11_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_MEMORY_0_11_CSRCFILE)

$(PK_BISCUITOS_MEMORY_0_11_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_MEMORY_0_11_sh,$(srctree))
