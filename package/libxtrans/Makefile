################################################
#
# xtrans
#
################################################
LIB_LIBXTRANS_NAME	:= xtrans
LIB_LIBXTRANS_VERSION	:= $(CONFIG_LIB_LIBXTRANS_VERSION)
LIB_LIBXTRANS_SITE	:= $(CONFIG_LIB_LIBXTRANS_URL)
LIB_LIBXTRANS_GITHUB	:= 
LIB_LIBXTRANS_SUBNAME	:= 
LIB_LIBXTRANS_DIR	:= $(srctree)/package/xtrans
LIB_LIBXTRANS_PATCH	:= $(srctree)/package/xtrans/patch
LIB_LIBXTRANS_DEMO	:= $(srctree)/package/xtrans/demo.c
LIB_LIBXTRANS_OUT	:= 
LIB_LIBXTRANS_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBXTRANS_TAR	:= $(CONFIG_LIB_LIBXTRANS_TAR)
LIB_LIBXTRANS_PACKAGE	:= 
LIB_LIBXTRANS_CONFIG	:=
SUB_TARGET		+= $(LIB_LIBXTRANS_DIR)/build

LIB_LIBXTRANS_sh = $(CONFIG_SHELL) $(LIB_LIBXTRANS_SCRIPTS) \
               $1 $(LIB_LIBXTRANS_NAME)X $(LIB_LIBXTRANS_VERSION)X \
               $(LIB_LIBXTRANS_SITE)X $(LIB_LIBXTRANS_GITHUB)X \
               $(LIB_LIBXTRANS_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBXTRANS_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBXTRANS_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBXTRANS_SUBNAME)X \
               $(LIB_LIBXTRANS_CONFIG)X $(CONFIG_LIB_LIBXTRANS_CONFIG)X \
	       $(CONFIG_LIB_LIBXTRANS_LDFLAGS) $(CONFIG_LIB_LIBXTRANS_CFLAGS)

$(LIB_LIBXTRANS_DIR)/build: FORCE
	$(Q)$(call LIB_LIBXTRANS_sh,$(srctree))
