################################################
#
# Algorithm bindlist
#
################################################
ALGO_BINDLIST_NAME	:= bindlist
ALGO_BINDLIST_VERSION	:= $(CONFIG_ALGO_BINDLIST_VERSION)
ALGO_BINDLIST_SITE	:= $(CONFIG_ALGO_BINDLIST_URL)
ALGO_BINDLIST_GITHUB	:= 
ALGO_BINDLIST_SUBNAME	:= 
ALGO_BINDLIST_DIR		:= $(srctree)/package/algo_bindlist
ALGO_BINDLIST_PATCH	:= $(srctree)/package/algo_bindlist/patch
ALGO_BINDLIST_DEMO	:= $(srctree)/package/algo_bindlist/demo.c
ALGO_BINDLIST_OUT		:= 
ALGO_BINDLIST_SCRIPTS	:= $(srctree)/scripts/package/GNU-app.sh
ALGO_BINDLIST_TAR		:= $(CONFIG_ALGO_BINDLIST_TAR)
ALGO_BINDLIST_PACKAGE	:= 
ALGO_BINDLIST_CONFIG	:=
SUB_TARGET		+= $(ALGO_BINDLIST_DIR)/build

ALGO_BINDLIST_sh = $(CONFIG_SHELL) $(ALGO_BINDLIST_SCRIPTS) \
               $1 $(ALGO_BINDLIST_NAME)X $(ALGO_BINDLIST_VERSION)X \
               $(ALGO_BINDLIST_SITE)X $(ALGO_BINDLIST_GITHUB)X \
               $(ALGO_BINDLIST_PATCH)X 5.0 \
	       $(CONFIG_ALGO_BINDLIST_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(ALGO_BINDLIST_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(ALGO_BINDLIST_SUBNAME)X \
               $(ALGO_BINDLIST_CONFIG)X $(CONFIG_ALGO_BINDLIST_CONFIG)X \
	       $(CONFIG_ALGO_BINDLIST_LDFLAGS)X $(CONFIG_ALGO_BINDLIST_CFLAGS)X \
	       $(CONFIG_ALGO_BINDLIST_SRC)X

$(ALGO_BINDLIST_DIR)/build: FORCE
	$(Q)$(call ALGO_BINDLIST_sh,$(srctree))
