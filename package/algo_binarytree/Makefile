################################################
#
# Algorithm binarytree
#
################################################
ALGO_BINARYTREE_NAME	:= binarytree
ALGO_BINARYTREE_VERSION	:= $(CONFIG_ALGO_BINARYTREE_VERSION)
ALGO_BINARYTREE_SITE	:= $(CONFIG_ALGO_BINARYTREE_URL)
ALGO_BINARYTREE_GITHUB	:= 
ALGO_BINARYTREE_SUBNAME	:= 
ALGO_BINARYTREE_DIR		:= $(srctree)/package/algo_binarytree
ALGO_BINARYTREE_PATCH	:= $(srctree)/package/algo_binarytree/patch
ALGO_BINARYTREE_DEMO	:= $(srctree)/package/algo_binarytree/demo.c
ALGO_BINARYTREE_OUT		:= 
ALGO_BINARYTREE_SCRIPTS	:= $(srctree)/scripts/package/GNU-app.sh
ALGO_BINARYTREE_TAR		:= $(CONFIG_ALGO_BINARYTREE_TAR)
ALGO_BINARYTREE_PACKAGE	:= 
ALGO_BINARYTREE_CONFIG	:=
SUB_TARGET		+= $(ALGO_BINARYTREE_DIR)/build

ALGO_BINARYTREE_sh = $(CONFIG_SHELL) $(ALGO_BINARYTREE_SCRIPTS) \
               $1 $(ALGO_BINARYTREE_NAME)X $(ALGO_BINARYTREE_VERSION)X \
               $(ALGO_BINARYTREE_SITE)X $(ALGO_BINARYTREE_GITHUB)X \
               $(ALGO_BINARYTREE_PATCH)X 5.0 \
	       $(CONFIG_ALGO_BINARYTREE_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(ALGO_BINARYTREE_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(ALGO_BINARYTREE_SUBNAME)X \
               $(ALGO_BINARYTREE_CONFIG)X $(CONFIG_ALGO_BINARYTREE_CONFIG)X \
	       $(CONFIG_ALGO_BINARYTREE_LDFLAGS)X $(CONFIG_ALGO_BINARYTREE_CFLAGS)X \
	       $(CONFIG_ALGO_BINARYTREE_SRC)X $(CONFIG_ALGO_BINARYTREE_CSRC)X

$(ALGO_BINARYTREE_DIR)/build: FORCE
	$(Q)$(call ALGO_BINARYTREE_sh,$(srctree))
