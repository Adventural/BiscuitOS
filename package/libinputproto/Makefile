################################################
#
# inputproto
#
################################################
LIB_LIBINPUTPROTO_NAME		:= inputproto
LIB_LIBINPUTPROTO_VERSION	:= $(CONFIG_LIB_LIBINPUTPROTO_VERSION)
LIB_LIBINPUTPROTO_SITE		:= $(CONFIG_LIB_LIBINPUTPROTO_URL)
LIB_LIBINPUTPROTO_GITHUB	:= 
LIB_LIBINPUTPROTO_SUBNAME	:= 
LIB_LIBINPUTPROTO_DIR		:= $(srctree)/package/inputproto
LIB_LIBINPUTPROTO_PATCH		:= $(srctree)/package/inputproto/patch
LIB_LIBINPUTPROTO_DEMO		:= $(srctree)/package/inputproto/demo.c
LIB_LIBINPUTPROTO_OUT		:= 
LIB_LIBINPUTPROTO_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBINPUTPROTO_TAR		:= $(CONFIG_LIB_LIBINPUTPROTO_TAR)
LIB_LIBINPUTPROTO_PACKAGE	:= 
LIB_LIBINPUTPROTO_CONFIG	:=
SUB_TARGET			+= $(LIB_LIBINPUTPROTO_DIR)/build

LIB_LIBINPUTPROTO_sh = $(CONFIG_SHELL) $(LIB_LIBINPUTPROTO_SCRIPTS) \
               $1 $(LIB_LIBINPUTPROTO_NAME)X $(LIB_LIBINPUTPROTO_VERSION)X \
               $(LIB_LIBINPUTPROTO_SITE)X $(LIB_LIBINPUTPROTO_GITHUB)X \
               $(LIB_LIBINPUTPROTO_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBINPUTPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBINPUTPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBINPUTPROTO_SUBNAME)X \
               $(LIB_LIBINPUTPROTO_CONFIG)X $(CONFIG_LIB_LIBINPUTPROTO_CONFIG)X \
	       $(CONFIG_LIB_LIBINPUTPROTO_LDFLAGS) $(CONFIG_LIB_LIBINPUTPROTO_CFLAGS)

$(LIB_LIBINPUTPROTO_DIR)/build: FORCE
	$(Q)$(call LIB_LIBINPUTPROTO_sh,$(srctree))
