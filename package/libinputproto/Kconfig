	default "https://www.x.org/releases/X11R7.7/src/proto/" if X11R7_7
	default "https://www.x.org/releases/X11R7.4/src/proto/" if X11R7_4
	#default "https://www.x.org/releases/individual/proto/"
	string "url"

config LIB_LIBINPUTPROTO_SRC
	string "source code list"

config LIB_LIBINPUTPROTO_LDFLAGS
	string "LDFLAGS"

config LIB_LIBINPUTPROTO_CFLAGS
	string "CFLAGS"

config LIB_LIBINPUTPROTO_CONFIG
	string "configure"

endif # PK_LIBINPUTPROTO
