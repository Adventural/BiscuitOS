PK_VFS_KERNEL_READ_UFILE_NAME        := BiscuitOS-Kernel-read-Userspace-file
PK_VFS_KERNEL_READ_UFILE_VERSION     := $(CONFIG_PK_VFS_KERNEL_READ_UFILE_VERSION)
PK_VFS_KERNEL_READ_UFILE_SITE        := $(CONFIG_PK_VFS_KERNEL_READ_UFILE_URL)
PK_VFS_KERNEL_READ_UFILE_GITHUB      := $(CONFIG_PK_VFS_KERNEL_READ_UFILE_GITHUB)
PK_VFS_KERNEL_READ_UFILE_SUBNAME     :=
PK_VFS_KERNEL_READ_UFILE_BUILDARCH   := $(CONFIG_PK_VFS_KERNEL_READ_UFILE_BUILDARCH)
PK_VFS_KERNEL_READ_UFILE_DIR         := $(srctree)/package/VFS/Kernel-read-Userspace-file
PK_VFS_KERNEL_READ_UFILE_PATCH       := $(PK_VFS_KERNEL_READ_UFILE_DIR)/patch
PK_VFS_KERNEL_READ_UFILE_BSBIT       := BiscuitOS-Kernel-read-Userspace-file-$(PK_VFS_KERNEL_READ_UFILE_VERSION).bsbit
PK_VFS_KERNEL_READ_UFILE_OUT         :=
PK_VFS_KERNEL_READ_UFILE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_VFS_KERNEL_READ_UFILE_TAR         := $(CONFIG_PK_VFS_KERNEL_READ_UFILE_TAR)
PK_VFS_KERNEL_READ_UFILE_PACKAGE     :=
PK_VFS_KERNEL_READ_UFILE_CONFIG      := $(CONFIG_PK_VFS_KERNEL_READ_UFILE_CONFIG)
SUB_TARGET                  += $(PK_VFS_KERNEL_READ_UFILE_DIR)/build

PK_VFS_KERNEL_READ_UFILE_sh = $(CONFIG_SHELL) $(PK_VFS_KERNEL_READ_UFILE_SCRIPTS) \
                 $1X \
                 $(PK_VFS_KERNEL_READ_UFILE_NAME)X \
                 $(PK_VFS_KERNEL_READ_UFILE_VERSION)X \
                 $(PK_VFS_KERNEL_READ_UFILE_SITE)X \
                 $(PK_VFS_KERNEL_READ_UFILE_GITHUB)X \
                 $(PK_VFS_KERNEL_READ_UFILE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_VFS_KERNEL_READ_UFILE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_VFS_KERNEL_READ_UFILE_SUBNAME)X \
                 $(CONFIG_PK_VFS_KERNEL_READ_UFILE_CONFIG)X \
                 $(CONFIG_PK_VFS_KERNEL_READ_UFILE_LIBPATH)X \
                 $(CONFIG_PK_VFS_KERNEL_READ_UFILE_CPPFLAGS)X \
                 $(CONFIG_PK_VFS_KERNEL_READ_UFILE_DPKCONFIG)X \
                 $(CONFIG_PK_VFS_KERNEL_READ_UFILE_CFLAGS)X \
                 $(CONFIG_PK_VFS_KERNEL_READ_UFILE_LDFLAGS)X \
                 $(CONFIG_PK_VFS_KERNEL_READ_UFILE_CXXFLAGS)X \
                 $(CONFIG_PK_VFS_KERNEL_READ_UFILE_ASFLAGS)X \
                 $(PK_VFS_KERNEL_READ_UFILE_BSBIT)X \
                 $(PK_VFS_KERNEL_READ_UFILE_BUILDARCH)X \
		 $(PK_VFS_KERNEL_READ_UFILE_CSRCFILE)X \
                 $(CONFIG_PK_VFS_KERNEL_READ_UFILE_SRCFILE)X \
                 $(CONFIG_PK_VFS_KERNEL_READ_UFILE_CSRCFILE)

$(PK_VFS_KERNEL_READ_UFILE_DIR)/build: FORCE
	$(Q)$(call PK_VFS_KERNEL_READ_UFILE_sh,$(srctree))
