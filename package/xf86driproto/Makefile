################################################
#
# xf86driproto
#
################################################
XF86DRIPROTO_NAME		:= xf86driproto
XF86DRIPROTO_VERSION		:= $(CONFIG_XF86DRIPROTO_VERSION)
XF86DRIPROTO_SITE		:= $(CONFIG_XF86DRIPROTO_URL)
XF86DRIPROTO_GITHUB		:= 
XF86DRIPROTO_SUBNAME		:= 
XF86DRIPROTO_DIR		:= $(srctree)/package/xf86driproto
XF86DRIPROTO_PATCH		:= $(srctree)/package/xf86driproto/patch
XF86DRIPROTO_DEMO		:= $(srctree)/package/xf86driproto/demo.c
XF86DRIPROTO_OUT		:= 
XF86DRIPROTO_SCRIPTS		:= $(srctree)/scripts/package/GNU-package.sh
XF86DRIPROTO_TAR		:= $(CONFIG_XF86DRIPROTO_TAR)
XF86DRIPROTO_PACKAGE		:= 
XF86DRIPROTO_CONFIG		:=
SUB_TARGET			+= $(XF86DRIPROTO_DIR)/build

XF86DRIPROTO_sh = $(CONFIG_SHELL) $(XF86DRIPROTO_SCRIPTS) \
               $1 $(XF86DRIPROTO_NAME)X $(XF86DRIPROTO_VERSION)X \
               $(XF86DRIPROTO_SITE)X $(XF86DRIPROTO_GITHUB)X \
               $(XF86DRIPROTO_PATCH)X 5.0 \
	       $(CONFIG_XF86DRIPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(XF86DRIPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(XF86DRIPROTO_SUBNAME)X \
               $(XF86DRIPROTO_CONFIG)X $(CONFIG_XF86DRIPROTO_CONFIG)X \
	       $(CONFIG_XF86DRIPROTO_LDFLAGS) $(CONFIG_XF86DRIPROTO_CFLAGS)

$(XF86DRIPROTO_DIR)/build: FORCE
	$(Q)$(call XF86DRIPROTO_sh,$(srctree))
