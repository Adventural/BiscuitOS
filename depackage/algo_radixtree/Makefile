################################################
#
# Algorithm radix-tree
#
################################################
ALGO_RADIXTREE_NAME	:= radixtree
ALGO_RADIXTREE_VERSION	:= $(CONFIG_ALGO_RADIXTREE_VERSION)
ALGO_RADIXTREE_SITE	:= $(CONFIG_ALGO_RADIXTREE_URL)
ALGO_RADIXTREE_GITHUB	:= 
ALGO_RADIXTREE_SUBNAME	:= 
ALGO_RADIXTREE_DIR	:= $(srctree)/package/algo_radixtree
ALGO_RADIXTREE_PATCH	:= $(srctree)/package/algo_radixtree/patch
ALGO_RADIXTREE_DEMO	:= $(srctree)/package/algo_radixtree/demo.c
ALGO_RADIXTREE_OUT	:= 
ALGO_RADIXTREE_SCRIPTS	:= $(srctree)/scripts/package/GNU-app.sh
ALGO_RADIXTREE_TAR	:= $(CONFIG_ALGO_RADIXTREE_TAR)
ALGO_RADIXTREE_PACKAGE	:= 
ALGO_RADIXTREE_CONFIG	:=
SUB_TARGET		+= $(ALGO_RADIXTREE_DIR)/build

ALGO_RADIXTREE_sh = $(CONFIG_SHELL) $(ALGO_RADIXTREE_SCRIPTS) \
               $1 $(ALGO_RADIXTREE_NAME)X $(ALGO_RADIXTREE_VERSION)X \
               $(ALGO_RADIXTREE_SITE)X $(ALGO_RADIXTREE_GITHUB)X \
               $(ALGO_RADIXTREE_PATCH)X 5.0 \
	       $(CONFIG_ALGO_RADIXTREE_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(ALGO_RADIXTREE_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(ALGO_RADIXTREE_SUBNAME)X \
               $(ALGO_RADIXTREE_CONFIG)X $(CONFIG_ALGO_RADIXTREE_CONFIG)X \
	       $(CONFIG_ALGO_RADIXTREE_LDFLAGS)X $(CONFIG_ALGO_RADIXTREE_CFLAGS)X \
	       $(CONFIG_ALGO_RADIXTREE_SRC)X $(CONFIG_ALGO_RADIXTREE_CSRC)X

$(ALGO_RADIXTREE_DIR)/build: FORCE
	$(Q)$(call ALGO_RADIXTREE_sh,$(srctree))
