################################################
#
# compositeproto
#
################################################
COMPOSITEPROTO_NAME		:= compositeproto
COMPOSITEPROTO_VERSION		:= $(CONFIG_COMPOSITEPROTO_VERSION)
COMPOSITEPROTO_SITE		:= $(CONFIG_COMPOSITEPROTO_URL)
COMPOSITEPROTO_GITHUB		:= 
COMPOSITEPROTO_SUBNAME		:= 
COMPOSITEPROTO_DIR		:= $(srctree)/package/compositeproto
COMPOSITEPROTO_PATCH		:= $(srctree)/package/compositeproto/patch
COMPOSITEPROTO_DEMO		:= $(srctree)/package/compositeproto/demo.c
COMPOSITEPROTO_OUT		:= 
COMPOSITEPROTO_SCRIPTS		:= $(srctree)/scripts/package/GNU-package.sh
COMPOSITEPROTO_TAR		:= $(CONFIG_COMPOSITEPROTO_TAR)
COMPOSITEPROTO_PACKAGE		:= 
COMPOSITEPROTO_CONFIG		:=
SUB_TARGET		+= $(COMPOSITEPROTO_DIR)/build

COMPOSITEPROTO_sh = $(CONFIG_SHELL) $(COMPOSITEPROTO_SCRIPTS) \
               $1 $(COMPOSITEPROTO_NAME)X $(COMPOSITEPROTO_VERSION)X \
               $(COMPOSITEPROTO_SITE)X $(COMPOSITEPROTO_GITHUB)X \
               $(COMPOSITEPROTO_PATCH)X 5.0 \
	       $(CONFIG_COMPOSITEPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(COMPOSITEPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(COMPOSITEPROTO_SUBNAME)X \
               $(COMPOSITEPROTO_CONFIG)X $(CONFIG_COMPOSITEPROTO_CONFIG)X \
	       $(CONFIG_COMPOSITEPROTO_LDFLAGS) $(CONFIG_COMPOSITEPROTO_CFLAGS)

$(COMPOSITEPROTO_DIR)/build: FORCE
	$(Q)$(call COMPOSITEPROTO_sh,$(srctree))
