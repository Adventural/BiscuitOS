################################################
#
# renderproto
#
################################################
RENDERPROTO_NAME		:= renderproto
RENDERPROTO_VERSION		:= $(CONFIG_RENDERPROTO_VERSION)
RENDERPROTO_SITE		:= $(CONFIG_RENDERPROTO_URL)
RENDERPROTO_GITHUB		:= 
RENDERPROTO_SUBNAME		:= 
RENDERPROTO_DIR			:= $(srctree)/package/renderproto
RENDERPROTO_PATCH		:= $(srctree)/package/renderproto/patch
RENDERPROTO_DEMO		:= $(srctree)/package/renderproto/demo.c
RENDERPROTO_OUT			:= 
RENDERPROTO_SCRIPTS		:= $(srctree)/scripts/package/GNU-package.sh
RENDERPROTO_TAR			:= $(CONFIG_RENDERPROTO_TAR)
RENDERPROTO_PACKAGE		:= 
RENDERPROTO_CONFIG		:=
SUB_TARGET			+= $(RENDERPROTO_DIR)/build

RENDERPROTO_sh = $(CONFIG_SHELL) $(RENDERPROTO_SCRIPTS) \
               $1 $(RENDERPROTO_NAME)X $(RENDERPROTO_VERSION)X \
               $(RENDERPROTO_SITE)X $(RENDERPROTO_GITHUB)X \
               $(RENDERPROTO_PATCH)X 5.0 \
	       $(CONFIG_RENDERPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(RENDERPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(RENDERPROTO_SUBNAME)X \
               $(RENDERPROTO_CONFIG)X $(CONFIG_RENDERPROTO_CONFIG)X \
	       $(CONFIG_RENDERPROTO_LDFLAGS) $(CONFIG_RENDERPROTO_CFLAGS)

$(RENDERPROTO_DIR)/build: FORCE
	$(Q)$(call RENDERPROTO_sh,$(srctree))
