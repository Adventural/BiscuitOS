################################################
#
# libXfixes
#
################################################
LIB_LIBXFIXES_NAME	:= libXfixes
LIB_LIBXFIXES_VERSION	:= $(CONFIG_LIB_LIBXFIXES_VERSION)
LIB_LIBXFIXES_SITE	:= $(CONFIG_LIB_LIBXFIXES_URL)
LIB_LIBXFIXES_GITHUB	:= 
LIB_LIBXFIXES_SUBNAME	:= 
LIB_LIBXFIXES_DIR	:= $(srctree)/package/libXfixes
LIB_LIBXFIXES_PATCH	:= $(srctree)/package/libXfixes/patch
LIB_LIBXFIXES_DEMO	:= $(srctree)/package/libXfixes/demo.c
LIB_LIBXFIXES_OUT	:= 
LIB_LIBXFIXES_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBXFIXES_TAR	:= $(CONFIG_LIB_LIBXFIXES_TAR)
LIB_LIBXFIXES_PACKAGE	:= 
LIB_LIBXFIXES_CONFIG	:=
SUB_TARGET		+= $(LIB_LIBXFIXES_DIR)/build

LIB_LIBXFIXES_sh = $(CONFIG_SHELL) $(LIB_LIBXFIXES_SCRIPTS) \
               $1 $(LIB_LIBXFIXES_NAME)X $(LIB_LIBXFIXES_VERSION)X \
               $(LIB_LIBXFIXES_SITE)X $(LIB_LIBXFIXES_GITHUB)X \
               $(LIB_LIBXFIXES_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBXFIXES_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBXFIXES_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBXFIXES_SUBNAME)X \
               $(LIB_LIBXFIXES_CONFIG)X $(CONFIG_LIB_LIBXFIXES_CONFIG)X \
	       $(CONFIG_LIB_LIBXFIXES_LDFLAGS) $(CONFIG_LIB_LIBXFIXES_CFLAGS)

$(LIB_LIBXFIXES_DIR)/build: FORCE
	$(Q)$(call LIB_LIBXFIXES_sh,$(srctree))
