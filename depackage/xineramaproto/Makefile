################################################
#
# xineramaproto
#
################################################
LIB_XINERAMAPROTO_NAME		:= xineramaproto
LIB_XINERAMAPROTO_VERSION	:= $(CONFIG_LIB_XINERAMAPROTO_VERSION)
LIB_XINERAMAPROTO_SITE		:= $(CONFIG_LIB_XINERAMAPROTO_URL)
LIB_XINERAMAPROTO_GITHUB	:= 
LIB_XINERAMAPROTO_SUBNAME	:= 
LIB_XINERAMAPROTO_DIR		:= $(srctree)/package/xineramaproto
LIB_XINERAMAPROTO_PATCH		:= $(srctree)/package/xineramaproto/patch
LIB_XINERAMAPROTO_DEMO		:= $(srctree)/package/xineramaproto/demo.c
LIB_XINERAMAPROTO_OUT		:= 
LIB_XINERAMAPROTO_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_XINERAMAPROTO_TAR		:= $(CONFIG_LIB_XINERAMAPROTO_TAR)
LIB_XINERAMAPROTO_PACKAGE	:= 
LIB_XINERAMAPROTO_CONFIG	:=
SUB_TARGET			+= $(LIB_XINERAMAPROTO_DIR)/build

LIB_XINERAMAPROTO_sh = $(CONFIG_SHELL) $(LIB_XINERAMAPROTO_SCRIPTS) \
               $1 $(LIB_XINERAMAPROTO_NAME)X $(LIB_XINERAMAPROTO_VERSION)X \
               $(LIB_XINERAMAPROTO_SITE)X $(LIB_XINERAMAPROTO_GITHUB)X \
               $(LIB_XINERAMAPROTO_PATCH)X 5.0 \
	       $(CONFIG_LIB_XINERAMAPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_XINERAMAPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_XINERAMAPROTO_SUBNAME)X \
               $(LIB_XINERAMAPROTO_CONFIG)X $(CONFIG_LIB_XINERAMAPROTO_CONFIG)X \
	       $(CONFIG_LIB_XINERAMAPROTO_LDFLAGS) $(CONFIG_LIB_XINERAMAPROTO_CFLAGS)

$(LIB_XINERAMAPROTO_DIR)/build: FORCE
	$(Q)$(call LIB_XINERAMAPROTO_sh,$(srctree))
