menuconfig PK_GNU_GAWK
	bool "GNU - gawk"

if PK_GNU_GAWK

config GNU_GAWK_VERSION
	default "5.0.0"
	string "verstion"

config GNU_GAWK_TAR
	default "tar.gz"
	string "tar type"

config GNU_GAWK_URL
	default "http://ftp.gnu.org/pub/gnu/gawk/"
	string "url"

config GNU_GAWK_LDFLAGS
	string "LDFLAGS"

config GNU_GAWK_CFLAGS
	string "CFLAGS"

config GNU_GAWK_CONFIG
	string "configure"

endif # PK_GNU_GAWK
