################################################
#
# resourceproto
#
################################################
RESOURCEPROTO_NAME		:= resourceproto
RESOURCEPROTO_VERSION		:= $(CONFIG_RESOURCEPROTO_VERSION)
RESOURCEPROTO_SITE		:= $(CONFIG_RESOURCEPROTO_URL)
RESOURCEPROTO_GITHUB		:= 
RESOURCEPROTO_SUBNAME		:= 
RESOURCEPROTO_DIR		:= $(srctree)/package/resourceproto
RESOURCEPROTO_PATCH		:= $(srctree)/package/resourceproto/patch
RESOURCEPROTO_DEMO		:= $(srctree)/package/resourceproto/demo.c
RESOURCEPROTO_OUT		:= 
RESOURCEPROTO_SCRIPTS		:= $(srctree)/scripts/package/GNU-package.sh
RESOURCEPROTO_TAR		:= $(CONFIG_RESOURCEPROTO_TAR)
RESOURCEPROTO_PACKAGE		:= 
RESOURCEPROTO_CONFIG		:=
SUB_TARGET			+= $(RESOURCEPROTO_DIR)/build

RESOURCEPROTO_sh = $(CONFIG_SHELL) $(RESOURCEPROTO_SCRIPTS) \
               $1 $(RESOURCEPROTO_NAME)X $(RESOURCEPROTO_VERSION)X \
               $(RESOURCEPROTO_SITE)X $(RESOURCEPROTO_GITHUB)X \
               $(RESOURCEPROTO_PATCH)X 5.0 \
	       $(CONFIG_RESOURCEPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(RESOURCEPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(RESOURCEPROTO_SUBNAME)X \
               $(RESOURCEPROTO_CONFIG)X $(CONFIG_RESOURCEPROTO_CONFIG)X \
	       $(CONFIG_RESOURCEPROTO_LDFLAGS) $(CONFIG_RESOURCEPROTO_CFLAGS)

$(RESOURCEPROTO_DIR)/build: FORCE
	$(Q)$(call RESOURCEPROTO_sh,$(srctree))
