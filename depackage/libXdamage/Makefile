################################################
#
# libXdamage
#
################################################
LIB_LIBXDAMAGE_NAME	:= libXdamage
LIB_LIBXDAMAGE_VERSION	:= $(CONFIG_LIB_LIBXDAMAGE_VERSION)
LIB_LIBXDAMAGE_SITE	:= $(CONFIG_LIB_LIBXDAMAGE_URL)
LIB_LIBXDAMAGE_GITHUB	:= 
LIB_LIBXDAMAGE_SUBNAME	:= 
LIB_LIBXDAMAGE_DIR	:= $(srctree)/package/libXdamage
LIB_LIBXDAMAGE_PATCH	:= $(srctree)/package/libXdamage/patch
LIB_LIBXDAMAGE_DEMO	:= $(srctree)/package/libXdamage/demo.c
LIB_LIBXDAMAGE_OUT	:= 
LIB_LIBXDAMAGE_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBXDAMAGE_TAR	:= $(CONFIG_LIB_LIBXDAMAGE_TAR)
LIB_LIBXDAMAGE_PACKAGE	:= 
LIB_LIBXDAMAGE_CONFIG	:=
SUB_TARGET		+= $(LIB_LIBXDAMAGE_DIR)/build

LIB_LIBXDAMAGE_sh = $(CONFIG_SHELL) $(LIB_LIBXDAMAGE_SCRIPTS) \
               $1 $(LIB_LIBXDAMAGE_NAME)X $(LIB_LIBXDAMAGE_VERSION)X \
               $(LIB_LIBXDAMAGE_SITE)X $(LIB_LIBXDAMAGE_GITHUB)X \
               $(LIB_LIBXDAMAGE_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBXDAMAGE_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBXDAMAGE_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBXDAMAGE_SUBNAME)X \
               $(LIB_LIBXDAMAGE_CONFIG)X $(CONFIG_LIB_LIBXDAMAGE_CONFIG)X \
	       $(CONFIG_LIB_LIBXDAMAGE_LDFLAGS) $(CONFIG_LIB_LIBXDAMAGE_CFLAGS)

$(LIB_LIBXDAMAGE_DIR)/build: FORCE
	$(Q)$(call LIB_LIBXDAMAGE_sh,$(srctree))
