################################################
#
# libXcursor
#
################################################
LIB_LIBXCURSOR_NAME	:= libXcursor
LIB_LIBXCURSOR_VERSION	:= $(CONFIG_LIB_LIBXCURSOR_VERSION)
LIB_LIBXCURSOR_SITE	:= $(CONFIG_LIB_LIBXCURSOR_URL)
LIB_LIBXCURSOR_GITHUB	:= 
LIB_LIBXCURSOR_SUBNAME	:= 
LIB_LIBXCURSOR_DIR	:= $(srctree)/package/libXcursor
LIB_LIBXCURSOR_PATCH	:= $(srctree)/package/libXcursor/patch
LIB_LIBXCURSOR_DEMO	:= $(srctree)/package/libXcursor/demo.c
LIB_LIBXCURSOR_OUT	:= 
LIB_LIBXCURSOR_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBXCURSOR_TAR	:= $(CONFIG_LIB_LIBXCURSOR_TAR)
LIB_LIBXCURSOR_PACKAGE	:= 
LIB_LIBXCURSOR_CONFIG	:=
SUB_TARGET		+= $(LIB_LIBXCURSOR_DIR)/build

LIB_LIBXCURSOR_sh = $(CONFIG_SHELL) $(LIB_LIBXCURSOR_SCRIPTS) \
               $1 $(LIB_LIBXCURSOR_NAME)X $(LIB_LIBXCURSOR_VERSION)X \
               $(LIB_LIBXCURSOR_SITE)X $(LIB_LIBXCURSOR_GITHUB)X \
               $(LIB_LIBXCURSOR_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBXCURSOR_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBXCURSOR_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBXCURSOR_SUBNAME)X \
               $(LIB_LIBXCURSOR_CONFIG)X $(CONFIG_LIB_LIBXCURSOR_CONFIG)X \
	       $(CONFIG_LIB_LIBXCURSOR_LDFLAGS) $(CONFIG_LIB_LIBXCURSOR_CFLAGS)

$(LIB_LIBXCURSOR_DIR)/build: FORCE
	$(Q)$(call LIB_LIBXCURSOR_sh,$(srctree))
