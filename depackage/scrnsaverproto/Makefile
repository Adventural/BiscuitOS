################################################
#
# scrnsaverproto
#
################################################
SCRNSAVERPROTO_NAME		:= scrnsaverproto
SCRNSAVERPROTO_VERSION		:= $(CONFIG_SCRNSAVERPROTO_VERSION)
SCRNSAVERPROTO_SITE		:= $(CONFIG_SCRNSAVERPROTO_URL)
SCRNSAVERPROTO_GITHUB		:= 
SCRNSAVERPROTO_SUBNAME		:= 
SCRNSAVERPROTO_DIR		:= $(srctree)/package/scrnsaverproto
SCRNSAVERPROTO_PATCH		:= $(srctree)/package/scrnsaverproto/patch
SCRNSAVERPROTO_DEMO		:= $(srctree)/package/scrnsaverproto/demo.c
SCRNSAVERPROTO_OUT		:= 
SCRNSAVERPROTO_SCRIPTS		:= $(srctree)/scripts/package/GNU-package.sh
SCRNSAVERPROTO_TAR		:= $(CONFIG_SCRNSAVERPROTO_TAR)
SCRNSAVERPROTO_PACKAGE		:= 
SCRNSAVERPROTO_CONFIG		:=
SUB_TARGET			+= $(SCRNSAVERPROTO_DIR)/build

SCRNSAVERPROTO_sh = $(CONFIG_SHELL) $(SCRNSAVERPROTO_SCRIPTS) \
               $1 $(SCRNSAVERPROTO_NAME)X $(SCRNSAVERPROTO_VERSION)X \
               $(SCRNSAVERPROTO_SITE)X $(SCRNSAVERPROTO_GITHUB)X \
               $(SCRNSAVERPROTO_PATCH)X 5.0 \
	       $(CONFIG_SCRNSAVERPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(SCRNSAVERPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(SCRNSAVERPROTO_SUBNAME)X \
               $(SCRNSAVERPROTO_CONFIG)X $(CONFIG_SCRNSAVERPROTO_CONFIG)X \
	       $(CONFIG_SCRNSAVERPROTO_LDFLAGS) $(CONFIG_SCRNSAVERPROTO_CFLAGS)

$(SCRNSAVERPROTO_DIR)/build: FORCE
	$(Q)$(call SCRNSAVERPROTO_sh,$(srctree))
