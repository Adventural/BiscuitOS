################################################
#
# libfontenc
#
################################################
LIB_LIBFONTENC_NAME		:= libfontenc
LIB_LIBFONTENC_VERSION		:= $(CONFIG_LIB_LIBFONTENC_VERSION)
LIB_LIBFONTENC_SITE		:= $(CONFIG_LIB_LIBFONTENC_URL)
LIB_LIBFONTENC_GITHUB		:= 
LIB_LIBFONTENC_SUBNAME		:= 
LIB_LIBFONTENC_DIR		:= $(srctree)/package/libfontenc
LIB_LIBFONTENC_PATCH		:= $(srctree)/package/libfontenc/patch
LIB_LIBFONTENC_DEMO		:= $(srctree)/package/libfontenc/demo.c
LIB_LIBFONTENC_OUT		:= 
LIB_LIBFONTENC_SCRIPTS		:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBFONTENC_TAR		:= $(CONFIG_LIB_LIBFONTENC_TAR)
LIB_LIBFONTENC_PACKAGE		:= 
LIB_LIBFONTENC_CONFIG		:=
SUB_TARGET			+= $(LIB_LIBFONTENC_DIR)/build

LIB_LIBFONTENC_sh = $(CONFIG_SHELL) $(LIB_LIBFONTENC_SCRIPTS) \
               $1 $(LIB_LIBFONTENC_NAME)X $(LIB_LIBFONTENC_VERSION)X \
               $(LIB_LIBFONTENC_SITE)X $(LIB_LIBFONTENC_GITHUB)X \
               $(LIB_LIBFONTENC_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBFONTENC_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBFONTENC_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBFONTENC_SUBNAME)X \
               $(LIB_LIBFONTENC_CONFIG)X $(CONFIG_LIB_LIBFONTENC_CONFIG)X \
	       $(CONFIG_LIB_LIBFONTENC_LDFLAGS) $(CONFIG_LIB_LIBFONTENC_CFLAGS)

$(LIB_LIBFONTENC_DIR)/build: FORCE
	$(Q)$(call LIB_LIBFONTENC_sh,$(srctree))
