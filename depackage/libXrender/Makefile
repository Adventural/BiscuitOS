################################################
#
# libXrender
#
################################################
LIB_LIBXRENDER_NAME	:= libXrender
LIB_LIBXRENDER_VERSION	:= $(CONFIG_LIB_LIBXRENDER_VERSION)
LIB_LIBXRENDER_SITE	:= $(CONFIG_LIB_LIBXRENDER_URL)
LIB_LIBXRENDER_GITHUB	:= 
LIB_LIBXRENDER_SUBNAME	:= 
LIB_LIBXRENDER_DIR	:= $(srctree)/package/libXrender
LIB_LIBXRENDER_PATCH	:= $(srctree)/package/libXrender/patch
LIB_LIBXRENDER_DEMO	:= $(srctree)/package/libXrender/demo.c
LIB_LIBXRENDER_OUT	:= 
LIB_LIBXRENDER_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBXRENDER_TAR	:= $(CONFIG_LIB_LIBXRENDER_TAR)
LIB_LIBXRENDER_PACKAGE	:= 
LIB_LIBXRENDER_CONFIG	:=
SUB_TARGET		+= $(LIB_LIBXRENDER_DIR)/build

LIB_LIBXRENDER_sh = $(CONFIG_SHELL) $(LIB_LIBXRENDER_SCRIPTS) \
               $1 $(LIB_LIBXRENDER_NAME)X $(LIB_LIBXRENDER_VERSION)X \
               $(LIB_LIBXRENDER_SITE)X $(LIB_LIBXRENDER_GITHUB)X \
               $(LIB_LIBXRENDER_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBXRENDER_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBXRENDER_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBXRENDER_SUBNAME)X \
               $(LIB_LIBXRENDER_CONFIG)X $(CONFIG_LIB_LIBXRENDER_CONFIG)X \
	       $(CONFIG_LIB_LIBXRENDER_LDFLAGS) $(CONFIG_LIB_LIBXRENDER_CFLAGS)

$(LIB_LIBXRENDER_DIR)/build: FORCE
	$(Q)$(call LIB_LIBXRENDER_sh,$(srctree))
