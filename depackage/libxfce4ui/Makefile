################################################
#
# libxfce4ui
#
################################################
LIB_LIBXFCE4UI_NAME		:= libxfce4ui
LIB_LIBXFCE4UI_VERSION		:= $(CONFIG_LIB_LIBXFCE4UI_VERSION)
LIB_LIBXFCE4UI_SITE		:= $(CONFIG_LIB_LIBXFCE4UI_URL)
LIB_LIBXFCE4UI_GITHUB		:= 
LIB_LIBXFCE4UI_SUBNAME		:= 
LIB_LIBXFCE4UI_DIR		:= $(srctree)/package/libxfce4ui
LIB_LIBXFCE4UI_PATCH		:= $(srctree)/package/libxfce4ui/patch
LIB_LIBXFCE4UI_DEMO		:= $(srctree)/package/libxfce4ui/demo.c
LIB_LIBXFCE4UI_OUT		:= 
LIB_LIBXFCE4UI_SCRIPTS		:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBXFCE4UI_TAR		:= $(CONFIG_LIB_LIBXFCE4UI_TAR)
LIB_LIBXFCE4UI_PACKAGE		:= 
LIB_LIBXFCE4UI_CONFIG		:=
SUB_TARGET			+= $(LIB_LIBXFCE4UI_DIR)/build

LIB_LIBXFCE4UI_sh = $(CONFIG_SHELL) $(LIB_LIBXFCE4UI_SCRIPTS) \
               $1 $(LIB_LIBXFCE4UI_NAME)X $(LIB_LIBXFCE4UI_VERSION)X \
               $(LIB_LIBXFCE4UI_SITE)X $(LIB_LIBXFCE4UI_GITHUB)X \
               $(LIB_LIBXFCE4UI_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBXFCE4UI_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBXFCE4UI_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBXFCE4UI_SUBNAME)X \
               $(LIB_LIBXFCE4UI_CONFIG)X $(CONFIG_LIB_LIBXFCE4UI_CONFIG)X \
	       $(CONFIG_LIB_LIBXFCE4UI_LDFLAGS) $(CONFIG_LIB_LIBXFCE4UI_CFLAGS)

$(LIB_LIBXFCE4UI_DIR)/build: FORCE
	$(Q)$(call LIB_LIBXFCE4UI_sh,$(srctree))
