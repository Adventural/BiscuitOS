################################################
#
# xf86vidmodeproto
#
################################################
XF86VIDMODEPROTO_NAME		:= xf86vidmodeproto
XF86VIDMODEPROTO_VERSION	:= $(CONFIG_XF86VIDMODEPROTO_VERSION)
XF86VIDMODEPROTO_SITE		:= $(CONFIG_XF86VIDMODEPROTO_URL)
XF86VIDMODEPROTO_GITHUB		:= 
XF86VIDMODEPROTO_SUBNAME	:= 
XF86VIDMODEPROTO_DIR		:= $(srctree)/package/xf86vidmodeproto
XF86VIDMODEPROTO_PATCH		:= $(srctree)/package/xf86vidmodeproto/patch
XF86VIDMODEPROTO_DEMO		:= $(srctree)/package/xf86vidmodeproto/demo.c
XF86VIDMODEPROTO_OUT		:= 
XF86VIDMODEPROTO_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
XF86VIDMODEPROTO_TAR		:= $(CONFIG_XF86VIDMODEPROTO_TAR)
XF86VIDMODEPROTO_PACKAGE	:= 
XF86VIDMODEPROTO_CONFIG		:=
SUB_TARGET		+= $(XF86VIDMODEPROTO_DIR)/build

XF86VIDMODEPROTO_sh = $(CONFIG_SHELL) $(XF86VIDMODEPROTO_SCRIPTS) \
               $1 $(XF86VIDMODEPROTO_NAME)X $(XF86VIDMODEPROTO_VERSION)X \
               $(XF86VIDMODEPROTO_SITE)X $(XF86VIDMODEPROTO_GITHUB)X \
               $(XF86VIDMODEPROTO_PATCH)X 5.0 \
	       $(CONFIG_XF86VIDMODEPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(XF86VIDMODEPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(XF86VIDMODEPROTO_SUBNAME)X \
               $(XF86VIDMODEPROTO_CONFIG)X $(CONFIG_XF86VIDMODEPROTO_CONFIG)X \
	       $(CONFIG_XF86VIDMODEPROTO_LDFLAGS) $(CONFIG_XF86VIDMODEPROTO_CFLAGS)

$(XF86VIDMODEPROTO_DIR)/build: FORCE
	$(Q)$(call XF86VIDMODEPROTO_sh,$(srctree))
