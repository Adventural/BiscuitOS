################################################
#
# xextproto
#
################################################
LIB_LIBXEXTPROTO_NAME		:= xextproto
LIB_LIBXEXTPROTO_VERSION	:= $(CONFIG_LIB_LIBXEXTPROTO_VERSION)
LIB_LIBXEXTPROTO_SITE		:= $(CONFIG_LIB_LIBXEXTPROTO_URL)
LIB_LIBXEXTPROTO_GITHUB		:= 
LIB_LIBXEXTPROTO_SUBNAME	:= 
LIB_LIBXEXTPROTO_DIR		:= $(srctree)/package/xextproto
LIB_LIBXEXTPROTO_PATCH		:= $(srctree)/package/xextproto/patch
LIB_LIBXEXTPROTO_DEMO		:= $(srctree)/package/xextproto/demo.c
LIB_LIBXEXTPROTO_OUT		:= 
LIB_LIBXEXTPROTO_SCRIPTS	:= $(srctree)/scripts/package/GNU-package.sh
LIB_LIBXEXTPROTO_TAR		:= $(CONFIG_LIB_LIBXEXTPROTO_TAR)
LIB_LIBXEXTPROTO_PACKAGE	:= 
LIB_LIBXEXTPROTO_CONFIG		:=
SUB_TARGET			+= $(LIB_LIBXEXTPROTO_DIR)/build

LIB_LIBXEXTPROTO_sh = $(CONFIG_SHELL) $(LIB_LIBXEXTPROTO_SCRIPTS) \
               $1 $(LIB_LIBXEXTPROTO_NAME)X $(LIB_LIBXEXTPROTO_VERSION)X \
               $(LIB_LIBXEXTPROTO_SITE)X $(LIB_LIBXEXTPROTO_GITHUB)X \
               $(LIB_LIBXEXTPROTO_PATCH)X 5.0 \
	       $(CONFIG_LIB_LIBXEXTPROTO_SRC)X $(CONFIG_MAIN_PROJ_NAME)X \
               $(LIB_LIBXEXTPROTO_TAR) $(CONFIG_ARCH_MAGIC)X \
               $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X $(LIB_LIBXEXTPROTO_SUBNAME)X \
               $(LIB_LIBXEXTPROTO_CONFIG)X $(CONFIG_LIB_LIBXEXTPROTO_CONFIG)X \
	       $(CONFIG_LIB_LIBXEXTPROTO_LDFLAGS) $(CONFIG_LIB_LIBXEXTPROTO_CFLAGS)

$(LIB_LIBXEXTPROTO_DIR)/build: FORCE
	$(Q)$(call LIB_LIBXEXTPROTO_sh,$(srctree))
